import java.util.Scanner;

class Codechef
{
    public static void main(String[] args)
    {
        int a = 15;
        int b = 7;
        int c = 3;
        if (a > b)
        {
            System.out.println("A is greater than B");
        }
        else if (a > c)
        {
            System.out.println("A is greater than C");
        }
        else
        {
            System.out.println("None of it is true");
        }
    }
}
