import java.util.Scanner;

class Codechef
{
    public static void main(String[] args)
        {
            System.out.println("Multiplication Table of 2");
                    for (int i = 0; i <= 10; i++)
                    {
                        System.out.println("2 x "+ i + "= " + 2*i );
                        }
                    }
}
